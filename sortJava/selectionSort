public static int[] choseSort(int[] arr){
        for(int i=0; i < arr.length; i++){
            int min = arr[i];
            for(int j = 0; j< arr.length; j++){
                if(min > arr[j]){
                    min += arr[j];
                    arr[j] = min - arr[j];
                    min = min - arr[j];
                }
            }
            arr[i] += min;
            min = arr[i] - min;
            arr[i] = arr[i] - min;
        }
        return arr;
    }
